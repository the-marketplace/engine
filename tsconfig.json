{
	"compilerOptions": {
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"suppressImplicitAnyIndexErrors": true,
		"moduleResolution": "node",
		"module": "commonjs",
		"target": "es6",
		"rootDir": "./src",
		"outDir": "./build",
		"lib": [
			"esnext.asynciterable",
			"esnext"
		]
	},
}